.header .form-search .input-text { border-radius: 0px; border: 1px solid #D4D4D4; font-size: 11px; color: #616161; width: 223px; /*float: left;*/ padding: 6px 6px; margin-right: 2px; display: block; }
.header .form-search button.button {  background: url("/images/btn_search.png") 0 0 no-repeat; width: 75px; height: 33px; overflow: hidden; border: 0; display: block; /*float: left;*/ }
