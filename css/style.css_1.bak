/* Navigation
------------------------------------------------------------ */

.nav-wrapper { margin: 0px; }

#nav {
	border-top:1px solid #e5e5e5;
	padding:0;
	margin:0;
}

/* All Levels */
#nav li { text-align:left; position:relative; }
#nav li.hover { z-index:998; }
#nav > li.parent > a {
	background-image: url("/images/marker1.gif");
	background-repeat:no-repeat;
	background-position: 230px 15px;
}

#nav li.parent > a {

color: #000;
letter-spacing: 2px;
text-transform: uppercase;
text-rendering: optimizelegibility;
-webkit-font-smoothing: antialiased;
font-family: "ff-super-grotesk-web",helvetica,arial;
font-size: 12px;
font-weight: bold;
line-height: 1.4em;

}

#nav li a { display:block; text-decoration:none; }
#nav li a:hover { text-decoration:none; }
#nav li a span { display:block; white-space:nowrap; cursor:pointer; }
#nav li ul a span { white-space:normal; }

/* 1st Level */
#nav li {
	float:none;
	display:block;
	font-size:14px;
	line-height:17px;
	text-transform:uppercase;
	*float: none;
}
#nav li.items {}
#nav li.items.hover,
#nav li.items.active{}
#nav li a {
	background-color:#FFF;
	float:none;
	padding: 9px 0px 9px 20px;
	color:#0d0d0d;
	border-bottom:1px solid #e5e5e5;
	transition: all 0.3s ease;
	-o-transition: all 0.3s ease;
	-ms-transition: all 0.3s ease;
	-moz-transition: all 0.3s ease;
	-webkit-transition: all 0.3s ease;
	*width: 240px;
}
#nav li a:hover {
	background-color: #000;
	color:#FFF;
}
#nav li.last a {}
#nav li.hover a {}
#nav li.active > a { background-color:#ef0000;color:#fff; }
#nav > li.active > ul { display:block; *display:block; }

/* 2nd Level */
#nav div ul { position:static; width:auto; border:none; }

#nav ul {
    background: none repeat scroll 0 50% transparent;
    border: 0 none;
    display: none;
    left: 0;
    position: relative;
    top: 0;
    width: auto;
    *display: block;
}
#nav ul li {}
#nav ul li a:hover { background-color: #000; }
#nav ul li a {
	background: #FFF;
	font-size:11px;
	line-height:15px;
	color:#000;
	padding-left:40px;
	*display: block;
	*width: 220px;
	
	color: #000;
text-transform: uppercase;
letter-spacing: 1px;
text-rendering: optimizespeed;
-webkit-font-smoothing: subpixel-antialiased;
font-family: "ff-super-grotesk-web",helvetica,arial;
font-size: 13px;
font-weight: 400;
line-height: 1.4em;
display: block;

text-decoration: none;
border-bottom: 1px solid #ffac9c;
}
#nav ul li.last { border-bottom:0; }
#nav ul li.active > a {background-color:#5f5f5f;
}

#nav ul li.over > a { background-color:#5f5f5f; }

#nav > li > ul > li.items.hover,
#nav > li > ul > li.items.active{border:0;}

/* 3rd+ level */
#nav > li > ul > li > ul{position:relative; left:0!important; top:0; display:none; width:auto; border:0; background:0; zoom:1;}
#nav > li > ul > li.active > ul{display:block;}
#nav > li > ul > li > ul > li{border-bottom:0; }

#nav > li > ul > li > ul > li > ul > li{background:0!important;}

#nav ul ul {
    background: none repeat scroll 0 50% transparent;
    border: 0 none;
    display: none;
    left: 0;
    position: relative;
    top: 0;
    width: auto;
}
#nav ul ul,
#nav ul div { top:7px; }
#nav ul ul li a {
	background-position: 47px 15px;
	padding-left:60px;
}

/* Show menu */
#nav ul ul ul.shown-sub {
	position:absolute;
	display:block;
	background:#F4F4F4;
	border:1px solid #e5e5e5;
	left:190px;
	width:200px;
}
#nav ul ul ul.shown-sub li {}
#nav ul ul ul.shown-sub li a:hover {  }
#nav ul ul ul.shown-sub li a {
	padding:10px 10px 10px 30px;
	background-position: 10px 16px;
}
#nav ul ul ul.shown-sub li.last a {border-bottom:none;}

